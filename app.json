{
  "expo": {
    "name": "gallery-app",
    "slug": "gallery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-sqlite",
      "expo-secure-store"
    ],
    "icon": "./assets/png-clipart-computer-icons-scalable-graphics-gallery-icon-angle-text-thumbnail.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/favi.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/png-clipart-computer-icons-scalable-graphics-gallery-icon-angle-text-thumbnail.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favi.png"
    }
  }
}
